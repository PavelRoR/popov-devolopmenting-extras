#video
	padding 100px 0
	background-color: #F4E7DD
	position relative
	&:before
		content: ''
		display block
		position absolute
		width 174px
		height: 174px
		border-radius: 50%
		border 2px solid #5C5C5C
		top -50px
		right -80px
.video-wrapper-video
	max-width 560px
	margin 20px auto 0
.video-text
	font-family: NS
	font-size: 23px
	margin-bottom: 25px
	span
		font-weight: bold