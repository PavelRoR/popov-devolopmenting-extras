#last
	padding 135px 0
	background-color: #F4E7DD
	text-align center
	@media screen and (max-width 991px)
		padding 95px 0
	@media screen and (max-width 386px)
		padding 75px 0
.last
	&-title
		font-family: RW
		font-size: 35px
		font-weight: bold
		@media screen and (max-width 560px)
			font-size: 28px
		@media screen and (max-width 386px)
			font-size: 24px
	&-text
		font-family: RW
		font-size: 21px
		font-weight: 600
		margin 30px auto 40px
		width 100%
		max-width 430px
		@media screen and (max-width 560px)
			font-size: 19px
			margin 20px auto 30px
		@media screen and (max-width 386px)
			font-size: 17px