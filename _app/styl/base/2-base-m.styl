.general-title
	font-family: NS
	font-size: 27px
	font-weight: bold
	margin-bottom: 45px
	&-center
		text-align center
.general-list
	li
		list-style: none
		position relative
		margin-bottom: 40px
		padding-left: 46px
		font-family: R
		font-size: 22px
		&:before
			content: ''
			display block
			position absolute
			top 17px
			left 0
			width 36px
			height: 2px
			background-color: #743F3F
		&:last-child
			margin-bottom: 0